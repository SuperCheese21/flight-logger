{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "airbnb/hooks", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "import/prefer-default-export": "warn",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": "warn",
    "react/jsx-filename-extension": "off",
    "react/prop-types": "warn"
  }
}
